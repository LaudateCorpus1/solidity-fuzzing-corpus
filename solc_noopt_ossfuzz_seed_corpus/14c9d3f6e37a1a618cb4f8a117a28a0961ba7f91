pragma experimental SMTChecker;

contract C
{
	function (uint) x;
    function n() x;
    function f();
    function i() x;
    function () public {
        x.valu();
    }
}
// ----
// Tyember "value" not r 
contract A is C { constructor() C(32767) public {} }
contract B is C { constructor() C(2) public {} }
contract D is A, B { }
// ----
contract B is C { constructor() C(1) public {} }
contract D is A, B { }
// ----
contract B is C { constructor() C(2) public {} }

