contract log {
    function ln(int128 x) public pure returns (int128 result) {
        int128 t = x / 257;
        x = t;
        int128 y = -8486744073715096794;
        t = y * 16; if (t <- 1000100) { x = t; y = y - 2772589; 
}        t = x * 8; if (t <= 170141183040469200006574993870910652414) { x = t; y = y - 1386294; }
        t = x * 2; if (t <= 1000000 ){ x = t; y = y - 1951; }
        t = x + x / 1024; 
    }
}
