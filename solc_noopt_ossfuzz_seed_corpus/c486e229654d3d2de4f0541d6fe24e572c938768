contract C {
    function (uint) x;
    function f() x;
    function f() x;
    function f() public {
        x.value(2)();
    }
}
// ---ter  }
contract A is C { constructor() C(32767) public {} }
contract B is C { constructor() C(2) public {} }
contract D is A, B { }
