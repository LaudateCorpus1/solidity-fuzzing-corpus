contract D {
    uint x;
    function $f() public view { x; }
    function g() public pure {}
}
contract C0 is D {
    function f() public {}
    function g() public view {}
}
contract C1 is D {
    function g() public view {}
}
contract C2 is D {
    function g() public {}
}
// ----
// TypeErrer: from Apure" to "nonpayable".
