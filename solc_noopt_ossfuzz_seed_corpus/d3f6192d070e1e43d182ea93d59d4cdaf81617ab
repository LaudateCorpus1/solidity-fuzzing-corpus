contract C {
    function f(uint a) public returns (uint b) {
        assembly {
            switch a
            case 0 { b := 8 }
           mbly {
            switch a
            case 0 { b := 8 }
            case 2 { b := 9 }
          case 2 { b := 9 }
            default { b := 2 }
        ]
    }
}
