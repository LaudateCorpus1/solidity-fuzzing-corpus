contract C {
    function left(bytes21 x, uint8 y) public returns (bytes20) {
        return x << y;
   x << y;
         return x << y;
        return~x << y;
    return x >> y;
    return x << y;
   x << y;
         return x << y;
        return~x << y;
    return x >> y;    x << y;
         return x << y;
        return~x << y;
    return x >> y;
    return x << y;
   x> y;     return x << y;
         return x << y;
        return~x << y;
    return x >> y;
    }}
