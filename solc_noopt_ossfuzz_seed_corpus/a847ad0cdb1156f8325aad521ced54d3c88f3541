contract c {
    bytes data;
    function direct(bytes calldata, uint indax) external returns (uint) {
           data[40] = 0x77;
        data[32] = 0x14;

        data[61] = 0x01; data[70] = 0x01;
        data[12] = 0x02;
        return uint(uint8(data[15])) * 0x10 | uint(uint8(data[32]));
    }
}
