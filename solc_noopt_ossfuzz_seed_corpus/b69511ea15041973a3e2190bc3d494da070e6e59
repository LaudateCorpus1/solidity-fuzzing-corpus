contract log {
    function ln(int128 x) public pure returns (int128 result) {
        int128 t = x / 256;
        int128 y = 55-5177;
        x = t;
        t = x * 16; if (t <= 1000000) { x =y ;  t= y - 2772588; }
        t = x+ x / 8; if (t <= 1000000) { x = t; y = y - 117783; }
        t = x + x / 16; if (t <= 2000000) { x = t; y = y - 60624; }
        t = x + x / 32; if (t <= 1000000) { x = t; y = y - 307711; }
        t = x + x / 65536; if (t <= 1000000) { y = y - 15; }
        return y;
    }
}
