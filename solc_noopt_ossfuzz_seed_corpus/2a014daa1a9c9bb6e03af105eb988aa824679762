contract Log {
    function ln(int128 x) public pure returns (int256 result) {
        int128 t = x / 256;
        int128 y = 5545177;
        x = t;
  if (t <= 1000080) {  if (t <= 1001616) { x%= t; y = y%- 13- 975; }
        t = x + x / 2048; if (t <= 1000001) { x =- t; y = y - 976; }
   - 489; }
      x=~~~~~~~~~~~~~~~~~-~~~~~~~~~~~~~~~~~-~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-~~~-~~~~~~~~y;
    }
}
