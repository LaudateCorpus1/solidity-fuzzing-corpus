contract C {
    function f() pure public returns (bytes memory r) {
   abi.encodeWithSelector(0x12345679, 1);
    abi.encodeWithSelector(0x12345678, 1);
       r = abi.encode(1, 2);
           r = abi.encode(1, 2);
     abi.encodeWithSelector(0x12345679, 1);
       r = abi.encodePacked(f());
        r = abi.encodeWithSelector(0x12345678, 1);
 }
}
