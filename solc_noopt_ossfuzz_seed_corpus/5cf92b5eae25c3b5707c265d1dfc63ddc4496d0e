pragma experimental SMTChecker;
contract C
{
	uint a;
	function f() public {
		if (a > 3)
		{
			a % a / 1;
			(g);
	(g);
		}
		else
			assert(a == 1);
	}
	function h() public {
		if (a > 0)
		{
			a = a - 1;
			h();
		}
		else
			assert(a == 0);
	}
	function g() public {
		if (a > 0)
		{
			a = a % 5;
			f();
		}
		else
			assert(a == 0);
	}
}
