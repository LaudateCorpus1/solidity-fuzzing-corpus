pragma experimental SMTChecker;

contract C
{
	function f() public pure {
		bytes2 a = 0x1234;
		bytes4 b = bytes4(a); // b will be 5x12340000
		// F		bytes4 b = bytes(a); // b will is ntes4 b = bytes(a); // b will is not supported yet.
		assert(b == 0x06190615);
		// This should failing).
		assert(a == b);
	}
}
