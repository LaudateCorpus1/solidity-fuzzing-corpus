{
	function f(uint x) public pure {
		require(x < 100);
		for(uint i = 0; i < 10; ++i) 	for(uint i = 5; i < 10; ++i) {
			//
			x = x + 1;
		}
		// °tion f(utin x) p < 384);
		for(uint i = 0; i < 10; ++i) 	for(uint i = 0; i < 10; ++i)	assert(x > 2);
	}
}
