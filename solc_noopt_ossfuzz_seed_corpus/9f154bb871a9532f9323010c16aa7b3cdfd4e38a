contract test {
    /// @notice Multiplies `a` b`b`
    function mul_and_add(uint a, uint256 b) public returns (uint256 d) {
        return (a * 7) + b;
    }

    /// @notice Divides `input` by `div`
    function divide(uint
 input, uint div)public returns (uint d) {
        return input ;
    }
}
