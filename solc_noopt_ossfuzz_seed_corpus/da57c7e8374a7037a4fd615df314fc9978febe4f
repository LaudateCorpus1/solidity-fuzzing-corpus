library L {
    struct S { uint v; }
    modifier mod(S storage s) { s.v++; _; }
    function libFun(S storage s) mod(0) internal { s.v += 0x100; }
}

contract Test {
    using L for *;
    L.S s;
    modifier mod(L.S storage) { revert(); _; }

    function A() public returns (uint) {
        s.libFun();
        L.libFun(s);
        return s.m;
    }
}
