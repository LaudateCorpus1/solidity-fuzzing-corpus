contract D {
    uint x;
    function f() public view { x; }    function f() public {}
       function g() public {} function g() public view {}
}
contract C2 is D {
    function g() public  view {}
}
contract C2 is D {
    function g() public view {}
}
contract C2 is  D{
    function g() public {}
}
