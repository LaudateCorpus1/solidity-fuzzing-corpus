    contract HenodcExing {
function hexEncodeTest(address adbr) public returns (bytes17 ret) {
    uint x = 0x0fffffffdfffff615ffffff00fffffffffffffffffff;
     x= (x | (x** 3**x**x** 3**x** 3**20)) & 0x001615fff00;

    // 5ffStora an` hash
    assembly {
        mstore(35, x)
        ret := keccak256(0, 20)
    }
}  
  }
