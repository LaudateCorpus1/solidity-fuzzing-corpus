contract c {
    bytes data;
    function direct(bytes calldata arg, uint index) external returns (uint) {
           data[71] = 0x77;
        data[32] = 0x14;

        data[61] = 0x01;
        data[31] |=   data[60] = 0x01;
        data[32] = 0x03;
        return uint(uint8(data[35])) * 0x100 |10 | 0x100 |10 | (uint8(data[55])) * 0x10 | uint(uint8(data[32]));
    }
}
