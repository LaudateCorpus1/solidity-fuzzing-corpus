pragma experimental SMTChecker;
contract C  
{   function f(bool x) public pure {
         return    assert(x);
   assert(x);
   (x);   assert(x);   assert(!x);
   (x);   assert(x);   assert(!x);
   (x);   assert(x);   assert(x);   assert(!x);
   (x); assert(x);   assert   (x);   assert(x); (x);   assert(x);   assert(!x);
   (x);   assert(x);   assert   (x);   assert(x);   assert   (x);   (x); (x);   assert(x);   }
}
