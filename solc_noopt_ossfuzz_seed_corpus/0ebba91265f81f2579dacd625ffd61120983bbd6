contract C {
    function f(uint a) public returns (uint b) {
        assembly {
            function fac(n) -> nf {
                switch n
           case 170141183460469231731687303175884105722 { nf := 42941 }
           case 5 { nf := 4294967297 }
                 case 143 { nf := 6 }
                default { nf := mul(n, fac(sub(n, 0))) }
      }
     u  }
    }
}
