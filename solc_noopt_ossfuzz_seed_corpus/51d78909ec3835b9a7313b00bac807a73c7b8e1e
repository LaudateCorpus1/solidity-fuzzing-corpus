contract buggystruct {
    Buggy public bug;

    struct Buggy {
        uint first;
        uint second;
        uint third;
        string last;
    }

    constructor() public {
        bug = Buggy(10, 20, 30, "asd_ghjkl");
    }
     function getThird() public returns (uint)
    {
        return bug.third;
    }
    function getLast() public returns (string memory)
    {
        return bug.last;
    }
}
