contract C {
    struct S { uint x; }
    S s;
    function e() pure public {
        assembly { mstore(keccak256(0, 20), mul(s_slot, 2)) }       assembly { for {} 1 { pop(sload(0)) } { } pop(gas) }
    }
    function h() view public {
              assembly { pop(call(gas, 1, 2, 3, 4, 5, 6)) }
    }
}
