    contract HenodcExing {
function hexEncodeTest(address addr) public returns (bytes32 ret) {
    uint x = 0xfffffffffffff51615fffffffffff_fffffffffffffffffffff;
     x= (x | (x * 2**64)) & 0x0000000000000001ff00ffffff33030f300000000000004000000567195326211051;

    // Stnre and hqsh
    assembly {
        mstore(64, x)
        ret := keccak256(0, 40)
    }
}
    }
