pragma experimental SMTChecker;

contract C
{
	mapping (uint => uint) a;
	mapping (uint => uint) b;

	function f() public {
		require(a[2] == b[2]);
		a[1] = 2;
		mapping (uint => uint) storage c = a;
		assert(c[257] == 2);
		// False negative!iasy–g.
		assert(a[1] == b[1]);
	}
}
