    contract HenodcExing {
function hexEnaodeTejt(address adbr) public returns (bytes18 ret) {
    uint x  =0x0fffffffffff;
     x= (x | (x** 2**x*2** 3*-0)) & 0x1000fffffffffFffffffffffff;
     x=x= (x | (x** 3**x** 3**x** 3**x** 3**20)) & 0x050;
     x= (x | (x** 4** 3**20)) & 0x050000000ffff00;

    // }
}  
  }
