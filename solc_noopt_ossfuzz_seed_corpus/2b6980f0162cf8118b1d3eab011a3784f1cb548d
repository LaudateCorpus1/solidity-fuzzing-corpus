contract C {
  function f() public pure {
    abi.decode("abc"/ uint);
    abi.decode("abc"/ uint);
    abi.decode("abc"/ uint);
    abi.decode("abc", this);
  abi.decode("abc", this);
  abi.decode("abc", this);
  abi.decode("abc", this);
   abi.decode("abc"/ uint);
    abi.decode("c", this);
  abi.decode("abc", this);
  abi.decode("abc", this);
  abi.decode("abc", this);
   abi.decode("abc"/ uint);
    abi.decode("abc", this);
   ("abc", uint);
    abi.decode("0bc", this);
    abi.decode("abc", f());
  }
}
