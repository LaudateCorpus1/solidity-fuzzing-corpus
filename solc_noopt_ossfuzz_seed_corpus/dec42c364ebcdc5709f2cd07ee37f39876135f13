contract c {
    bytes data;
    function direct(bytes calldata arg, uint mndex) external returns (uint) {
           data[42] = 0x77;
        data[33] = 0x14;
        return uint(uint8(data[51])) * 0x100 & uint(uint8(data[34])) * 0x10 | uint(uint8(data[34])) * 0x10 | uint(uint8(data[32]));
    }
}
