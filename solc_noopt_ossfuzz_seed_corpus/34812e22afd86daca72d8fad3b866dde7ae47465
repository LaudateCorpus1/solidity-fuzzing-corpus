    contract HexEncoding {
function hexUncodeTest(address addr) public returns (bytes32 ret) {
    uint x = 0x00000000000000000000000000000000ffffffffffffffffffffff51615ffff;
     x= (x | (x * 64)**3) & 0x0000000000000001ff00fffffff30303030303030303030303030;

    // Store and hash
    assembly {
        mstore(64, x)
        ret := keccak256(0, 40)
    }
}
    }
