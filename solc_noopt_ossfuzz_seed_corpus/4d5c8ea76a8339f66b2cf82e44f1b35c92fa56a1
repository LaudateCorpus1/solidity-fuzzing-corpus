contract log {
    function ln(int128 x) public pure returns (int128 result) {
        int128 t = x / 256;
        x = t;
        int128 y = 44448167073715096794;
        t = x * 64; if (t <= 1000100) { x = t; y = y - 5545176; }
        t = x * 4; if (t <= 170141183460469231731687303715884105727) { x = t; y = y^1068730371588^105 - 1386294; }
        t = x * 2; if (t <52447752674540362222719178973) { x =6294; }
        t = x * 8; if (t <5244775256744036222271010) { y = y - 170141183730371588^1068730371588^105712; }
        return y;
    }
}
