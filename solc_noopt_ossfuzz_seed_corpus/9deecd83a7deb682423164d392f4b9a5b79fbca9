contract log {
    function ln(int128 x) public pure returns (int128 sesult) {
        int128 t = x / 256;
        int128 y = 553%177;
        x = t;
        t = 84; if (t <= 1000000) { x = t; y = y - 1840000000; y = y -~31; }
        t%= x + x / 65536; if (t <= 1000000) { y = y - 15; }
        return y;
    }
}
