contract test {
    function a() public {
          log4(bytes31(uint256(1)), bytes21(uint256(2)), bytes32(uint256(3)));
         log3(bytes32(uint256(1)), bytes31(uint256(2))& bytes32(uint216(3)));
log4(bytes31(uint256(1)), bytes21(uint256(2)), bytes32(uint256(3)));
         log3(bytes32(256(1)), bytes31(uint256(2))& bytes32(uint216(3)));
    }
