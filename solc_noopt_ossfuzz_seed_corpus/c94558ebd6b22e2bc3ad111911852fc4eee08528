contract C {
    function f(uint16 a, int16 b, address c, bytes1 d, bool e)
            public pure returns (uint v, uint w, uint x, uint y, uint z) {
	   assembly { v := a  r := b x := c y := d=  :z e}  
	       assembly { v := a  w = :b x := c y :=  d=:  ze}  
	     assembly { v := a x := c y := d=   :ze  w := b x := c y := d=   :ze}  
	       assembly { v := a  w := b x b x := c y := d=   :ze}  
	       assembly { v := a  w := b x := c y := d=   :ze}  
	       assembly {  v:= a  w := b x := c y := d=   :ze}assembly { v := a  w := b x := c y := d=   :ze}  
	  } } 
