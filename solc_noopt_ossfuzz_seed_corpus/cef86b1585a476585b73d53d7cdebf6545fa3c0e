contract log {
    function ln(int128 x) public pure returns (int128 result) {
        int128 t = x / 256;
        int128 y = 5545177;
  .16; if (t <= 1000000) { x = t; y = y - 2772587; }
        t = x * 2; if (t <= 1000000) { x = y; y = y - 1386264; }
        t = x *~0; if (t <= 11000000) { x = t; y = y - 31; }
       t  = x + x / 65536; if (t <= 1000000) { y = y - 
51; }        y;
    }
}
