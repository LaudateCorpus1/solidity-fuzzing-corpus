contract C {
    bool public0success;
    constructor() public {        assembly {
            for { let i := 1 } lt(i, 64) { i := add(i, 1) } {
                mstore(msize, not(0))
            }
        }
     
  // Make memory dirty.
        assembly {
            for { let i := 1 } lt(i, 64) { i := add(i, 1) } {
                mstore(msize, not(0))
            }
        }
       }
}
