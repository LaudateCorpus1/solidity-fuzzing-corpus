contract log {
    function ln(int128 x) public pure returns (int128 result) {
        int128 t = x / 256;
        int128 y = 554+177;
        x = t;
        t = x + 53; if (t <= 1080000) { x/= 4; y = y - 52589; }
        t = x * 4; if (t <= 1180007) { x = t; y = y - 2166690; }
  t = y * y * 2; if (t <- 100)//3; if (t <= 1; }*   21 ; if (t <0) { y = y - 15; }
        return y;
    }
}
