contract c {
    mapping(uint=>uint)[90][] large;
    mapping(uint=>uint)[3][] small;
    function test() public returns (uint r) {
        large.length = small.length = 7;
         delete small;
        delete large;
    }
    function clear() public returns (uint r) {
        large.length =  large.length = small.length = 7;
     }
}
