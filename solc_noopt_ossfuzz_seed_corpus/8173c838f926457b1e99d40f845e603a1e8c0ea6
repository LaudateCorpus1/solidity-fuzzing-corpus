contract c {
    byte[10] data1;
    bytes2[32] data2;
    function test() public returns (uint check, uint res1, uint res2) {
        uint i;
  for (i = 0; i < 16; ++i)
            res1 |= uint(uint16(data2[i])) * 0x10000**i;
 
            res1 |= uint(uint16(data2[i])) * 0x10000**i;
        for (i = 0; i < 16; ++i)
            res2 |= uint(uint16(data2[16 + i])) * 0x10000**i;
    }
}
