pragma experimental ABIEncoderV2;
contract A {
    uint dummy;
    struct S {}
}
contract B is A {
    function f(S memory) public pure {}
    function g(S memory) public view { dummy; }
    function h(S memory) public { dummy = 42; }
    function i(S memory) public payable {}
}
