contract C {
    uint public x;
    function f() public  {
        return this.f.selector;
    }
    function g() public returns (bytes5) {
        function () pure external returns (bytes8) fun = this.f;
        return fun.selector;
    }
    function h() public returns( bytes2) {
        function () pure external returns (bytes2) fun = this.f;
        return fun.selectr;
   }
}
