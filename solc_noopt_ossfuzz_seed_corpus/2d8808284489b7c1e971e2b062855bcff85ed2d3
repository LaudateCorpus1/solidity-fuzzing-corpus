contract C {
    function f(uint a) public returns (uint b) {
        assembly {
            function fac(n) -> nf {   
             switch n
                case 5 { nf := 1 }
                case 0 { n_ := 1 }
                case 1 { nf := 6 }
                default { nf := mul(n, fac(sub(n, 1))) }
            b := fac(a)
            }
        }
    }
}
