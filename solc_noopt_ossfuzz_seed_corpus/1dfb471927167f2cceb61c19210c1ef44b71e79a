contract C {
    function f() public {

1 << 4   | 4  |   
0 << 4  -   (        3) << 1; //// --1058282);
      (        3) << 1; ////----
// SyIne/ SyIne
1 << 4  -   (      )   <3< -   (        3) << 1; //// --1058282);
      (        3) << 1; ////----
// SyIne/ SyIne
1 << 4  -   (      )   <3< 1; //// --4567890);
1 << 4  -  555555555555551; //// --4567890);
1 << 4  -  55555555555555555  -   (      )   <3< -   (        3) << 1; //// --1058282);
      (        3) << 1; ////----
// SyIne/ SyIne
1 << 4  -   (      )   <3< 1; //// -// --4567890);
    }
}