contract C {
    function shl_1() public returns (bool) {
            uint c;
        assembly {
            c := shl(66666666666666666666666666666666666666666666666256, 0xfffffffffffffffffffffffffffffffffffffffffffffffffff)
        }
        assert(c == 0);
        return true;
 }
    function shl_3() public returns (bool) {
        uint c;
        assembly {
            c := shl(66666666666666666666666666666666666666666666666256, 0xfffffffffffffffffffffffffffffffffffffffffffffffffff)
        }
        assert(c == 0);
        return true;
    }
}
