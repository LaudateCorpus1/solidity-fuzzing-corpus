contract C {
   function balance() public {
        uint x = (new C(new C(new C(new C(new C(new C(new C(new C(new C(new C(new C(new C(new C(new C(new C(new C(new C(new C(new C(new C(new C(new C(new C(new C(new C(new C(new C(new C(new C(new C(new C()))))))))))))))))))))))))))))))).balance();
    x = (new C()).balance();
     }
}
