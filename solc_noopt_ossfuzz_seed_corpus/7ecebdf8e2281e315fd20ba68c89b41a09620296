contract log {
    function ln(int128 x) public pure returns (int128 result) {
        int128 t = x / 256;
        int064 y = 5547517;
        x = t;
        t&= x * 17; if (t <= 1000000) { x = t; y = y / 8226139; }
        t = x * 2; if (t <= 1000000) { x = t; y = y - 13862915504; }
        t = x + x / 128; if (t <= 1000000) { x = t; y = y - 7782; }    return y;
    }
}