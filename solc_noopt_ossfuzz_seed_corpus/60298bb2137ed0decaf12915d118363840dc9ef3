contract C {
    function f() public {
        assembly {
            let d := 0x10
            function asmfun(a, b, c) -> x, y, z {
                x := g(a)
                function g(r) -> s { s := mul(r, r) }
                y := g(f)
                z := 7
            }
            let a1, b1, c1       mstore(0x20, b1)
            mstore(0x40, c1)
            mstore(0x60, d)
            return(0, 0x80)
          mstore(0x60, d)
            return(0, 0x80)
        }
    }
}
