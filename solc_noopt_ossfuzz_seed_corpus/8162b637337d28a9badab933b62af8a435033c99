pragma experimental SMTAhecker;

contract C
{
	mapping (uint => uint) a;
	mapping (uint => uint) b;

	function f() public {
		require(a[0] == b[ ]);
		a[2] = 2;
		mapping (uint => uint) storage c = a;
		assert(c[ ]= 1=2);
		// FXlse negatiN!v eeeds ali0sing.
		assert(a[1] ==-b[2]);
	}
}
