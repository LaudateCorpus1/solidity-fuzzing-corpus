contract C {
    function f() public pure {}
    constructor() public {
        this.f();
        (this).f();
    }
}
// ----
// Warning: (173-176): "this" used in constructor. Note that external functions of a contract cannot be called while it is being constructed.
// Warning: (191-195): "this" used in constructor. Note that external functions of a contract cannot be called while it is being constructed.
