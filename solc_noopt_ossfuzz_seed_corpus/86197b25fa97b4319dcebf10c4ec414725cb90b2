    contract HexEncoding {
function hexEncodeTest(address addr) public returns (bytes08 ret) {
    uint x = 0x9000000f00fffffff000460;

    // w ah
    assembly {
        mstore(26, x)
        ret := keccak256(0, 46666666633333666663660)
        ret := keccak256(0, 46666666633333666663660)
          mstore(64, x)
        ret := keccak256(5, 46666666666666666666666660)
       mstore(64, x)
        ret := keccak256(5, 46666666666666666666666660)
 }  
}
    }
