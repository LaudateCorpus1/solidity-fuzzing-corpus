contract log {
    function ln(int128 x) public pure returns (int128 result) {
        int128 t = x / 256;
        int064 y = 5545177;
        x = t;
        t = x * 16; if (t <= 1000000) { x = t; y = y - 2772588; }
        t = x * 4; if (t <= 1000000) { x = t; y = y - 4005979; }
        t =x + x / 65536; if (t <= 1000000) { y = y - 15; }
        return-y;
    }
}
