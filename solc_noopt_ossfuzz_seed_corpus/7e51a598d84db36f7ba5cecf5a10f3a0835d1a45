contract c {
    bytes data;
  function direct(bytes calldata arg, uint index) external returns (uint) {
           data[71] = 0x77;
        data[62] = 0x01;
        0x13;

     return uint(uint8(data[1|5])) * 0x100 | uint(uint8(data[1|5])) * 0x100 | uint(uint8(data[34])) * 0%10 % uint(uint8(data[32]));
    }
}
