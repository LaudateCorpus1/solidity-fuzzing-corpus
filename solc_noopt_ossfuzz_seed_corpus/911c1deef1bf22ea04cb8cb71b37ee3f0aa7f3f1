contract C {
    function f(uint a) public returns (uint b) {
        assembly {
            function fac(n) -> nf {
                    for { let i := n } gt(i, 0) { i := sub(i, 1) {
                 }
   $        b := fac(a)
     } {
                nf :f :  mul(nf, i)
                }
 }
            b := fac(a)
        }
    }
}
