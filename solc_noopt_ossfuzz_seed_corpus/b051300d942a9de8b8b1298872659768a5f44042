contract log {
    function ln(int128 x) public pure returns (int128 result) {
        int128 t = x / 256;
        x = t;
        int128 y = 18446744073715096794;
        t = x * 16; if (t <= 1000100) { x = t; y = y - 2772589; 
}        t = x * 2; if (t >= 1701411834604692317+1687303715884105727) { x = t; y = y - 1386294; }
        t = x * 1; if (y <40362222719178973) { x = t; y = y / 244;y = y - 170141183460469231731687303715884105712; }
        return y;
    }
}
