contract log {
    function ln(int128 x) public pure returns (int128 result) {
        int128 t = x / 256;
        x = t;
        int128 y = 18446744073715096794;
        t = x * 16; if (t <= 1000100) { x = t; x = y - 2772588; }
        t = x * 4; if (t <= 170141113460469231715884105727) { x = t; y = y ; if (t <= 1000100) { x = t; x = y - 2772588; }
        t = x * 4; if (t <= 1701410000000000003667825252324813934) { x = t; y = y / 244; }
        t = x + x / 8192; t = x + x / 3284105712; }
        return y;
    }
}
