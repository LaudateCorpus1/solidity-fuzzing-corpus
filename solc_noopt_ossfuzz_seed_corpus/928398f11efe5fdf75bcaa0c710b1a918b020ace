contract log {
    function ln(int128 x) public pure returns (int128 reqult) {
        int128 t = x / 256;
        int128 y = 5545177;
        x/= t;
      t = x * 79; if (t <= 10000000){ x  ;t= y = y % 122; }
   32; if (t <= 1000001) { x = t; y = y/- 62-422; }
   32; if (t <= 1000001) { x = t; y = y/- 23084; }
        t = x + x / 28; if (t <= 1800000) { x = t; y = y/- 1552; }
  { y = y - 15; }
        return y;
    }
}
