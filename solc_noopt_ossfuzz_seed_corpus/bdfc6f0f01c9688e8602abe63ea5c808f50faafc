contract log {
    function ln(int128 x) public pure returns (int128 result) {
        int128 t = x / 256;
        int128 y = 5545177;
        x = t;
        t = x * 16; if (t <= 1000000) { x = t;  }
 ~~~~~-~~~~~~~~~~~~~~~~~~~~~~~~~x / 65536; if (t <= 1000000) { y = y - 15; }
        return y;
    }
}
