pragma experimental SMTChecker;
contract C
{
    address a;
    bool b;
    uint c;
    function f(uint x) public {
        if (x == 0)
{
  a = 0x0000000000000000000000000010000000199999;
            b = false;
        }
        assert(a > 0x00000000000000000A0000000010000000000000 && b);
    }
}
