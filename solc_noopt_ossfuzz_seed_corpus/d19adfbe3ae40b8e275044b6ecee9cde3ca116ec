    uint x = 0x0ffdf016fffeff00E;

         x= (x | (x** 1**x** 2**2**x** 3**20)) & 0x0000000fff1ffff  ;   x= (x | (x** 1**x** 3**20)) & 0x000000;

         x= (x | (x** 2**x** 1**2**x** 3**21)) & 0x0000000fffbffffffffffff1ffff;
  x   = (x | uint8 (x** 3**x*
 3 hours **x** 2**2**x** 440)) & 0x00f0000001615ffffff00;

