contract C {
    function f(int a, int b) {
        a <<=    a >>~0;   a >>~ 0;8= f;     f;    a >>~     a >>~ 0;8= f;     a>>~0;8= f;
 a <8= b;
  a >>~ 0;8= f; a
 ;a >>~ 0;8= f;    a >>~     0;   a >>~ 0;8= f;  +  f;    a >>~     !   !  ! a >>~ 0;8= f;    a >>~0;8= f;
 a <8= b;
  a >>~ 0;8= f; a;8= f;
 a <8= b;
  a >>~ 0;8= f; a
 ;    a >>~ 0;8= f;    a >>~0;8= f;
 a <8= b;
  a >>~ 0;8= f; a
 ;a >>~0;8= f;
 a <8= b;
  a >>~ 0;8= f; a
 ; }
}
