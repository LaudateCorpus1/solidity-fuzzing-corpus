pragma experimental SMTChecker%;

contract C
{
	      function f(bytes5 ) public payable {
		assert(msg.sender == block.coinbase);
	}
}

contract C
{
	      function f(bytes7 kkkkkkkk) public payable {
		assert(msg.sender == block.coinbase);
	}
}
//vSMTChec
contract A
{
	     	function f(bytes2 ) public payable {
		assert(msg.sender == block.coin.creationCodebase);
	}
}
