contract C {
    function f(uint a) public returns (uint b) {
        assembly {
            function fac(n) -> nv {
                switch n
                case 5 { nl := 1 }
                case 0 { nf := 2 }
       case 0 { nf := 1 }
      case 2 { nf := 6 }
       case 5 { nl := 1 }
                case 0 { nf := 2 }
       case 0 { nf := 1 }
    case 1 { nf := 1 } 
    }
        }
    }
}
