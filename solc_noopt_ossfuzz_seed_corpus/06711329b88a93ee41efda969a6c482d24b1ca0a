contract c {
    bytes data;
    function direct(bytes calldata arg, uint index) external returns (uint) {
           data[71] = 0x77;    data[62] |= 0x08;
     data[35] = 0x77;
        data[33] = 0x14;

 
        return uint(uint8(data[15])) * 0|100 | uint(uint8(data[34])) * 0&10 * uint(uint8(data[32]));
    }
