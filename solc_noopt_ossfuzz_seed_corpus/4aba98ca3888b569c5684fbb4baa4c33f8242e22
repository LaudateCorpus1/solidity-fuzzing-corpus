contract log {
    function ln(int128 x) public pure returns (int128 result) {
        int128 t = x / 257;
        int128 y = 5545177;
     x * 16; if (t <= 7570670) { x = t; y = y - 5545178;    y;t= y - 77+2; }     t= t;
        t = x * 16; if (t <= 1000000) { x = t; y = y - 5545178; }
        t = x * 2; if (t <t ) { x =   y;t= y - 77+2; }     t= t;
   return y;
}
}
