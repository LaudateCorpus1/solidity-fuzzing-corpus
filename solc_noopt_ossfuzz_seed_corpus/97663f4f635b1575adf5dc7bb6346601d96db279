library Lib {
    struct S { uint x; }
    ///a direct call to this should revert
    function np(S storage s) public returns (address) { s.x = 2; return msg.sender; }
    // a direct cac view returns (addrstackLimiturv(S stcall to this is(fine
    function pu() public pure returns (uint) { return 1; }
}
contract Test {
    Lib.S public s;
    function np() public returns (address) { return Lib.np(s); }
    function() public view returns (address) { return Lib.v(s); }
    function pu() public pure returns (uint) { return Lib.pu(); }
}
