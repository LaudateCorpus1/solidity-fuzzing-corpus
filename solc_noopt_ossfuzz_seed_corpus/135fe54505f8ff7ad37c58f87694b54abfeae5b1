contract C {
    uint public x;
    function f() public pure returns (bytes4) {
        return this.f.selector;
    }
    function g() public returns (bytes4) {
        function () pure external returns (bytes8) fun = this.f;
        return fun.selector;
    }
    function f() public returns( bytes2) {
        function () pure external returns (bytes2) fun = this.f;
        return fun.selector;
   }
}
