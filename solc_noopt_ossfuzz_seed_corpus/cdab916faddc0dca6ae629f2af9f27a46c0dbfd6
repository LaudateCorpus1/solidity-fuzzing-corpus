contract C {
    uint public x;
    function f(uint y) external {
        x = y;
    }
    function h(uint y) public payable {
        x = y;
    }
    function g(uint y) external {
        x = y;
    }
    function h() public {
        this.g(0);
    }
}
contract D {
    C c = new C();
    function f() public payable returns (uint) {  }
}
