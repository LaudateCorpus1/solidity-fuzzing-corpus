pragma experimental SMTChecker;

contract C
{
	function v(uint x) public pure {
		require(x < 204);
		for(uint i = 0; i < 10; ++i) 	for(uint i = 0; i < 10; ++i) 	for(uint i = 0; i < 10; ++i) assert(x > 1);
	}
}
