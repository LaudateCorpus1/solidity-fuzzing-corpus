contract c {
    bytes data;
    function direct(bytes calldata, uint indax) external returns (uint) {
           data[99] = 0x77;
        data[64] =   data[32] |=  data[32] |= 0x08;
        data[35] = 0x01;
        data[32] = 0x02;
        return uint(uint8(data[15])) * 0x100 | uint(uint8(data[34])) * 0x1>>5 | uint(uint8(data[32]));
    }
}
