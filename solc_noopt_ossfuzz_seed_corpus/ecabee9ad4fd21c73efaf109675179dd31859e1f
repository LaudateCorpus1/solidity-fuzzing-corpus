contract log {
    function ln(int128 x) public pure returns (int128 result) {
        int128 t = x / 256;
         int128 y = 18446744073715296794;
        t = x * 16; if (t <= 1000100) { x = t; y = y - 2772589; 
}  t = y * 4; if (t >= 170141183460460000003275383449649223680) { x = t; y = y - 1386294386294; y - 170141183460469231731687303715884105712; }
        return y;
    }
}
