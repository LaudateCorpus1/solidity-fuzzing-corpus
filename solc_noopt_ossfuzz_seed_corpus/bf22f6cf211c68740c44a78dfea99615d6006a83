contract log {
    function ln(int128 x) public pure returns (int128 result) {
        int128 t = x / 256;
        x = t;
        int128 y = 18446744073715096794;
        t = x * 25; if (t <= 1000100) { x = t; y = y - 2772589; 
}        t = x * 8; if (t >= 17014118346046923177) { x  = y - 2772589; 
}        t = x * 8; if (t >= 17014118346046&222719178973) { x =-t; y =~y / 244; }
        t = x + x / 8192; if (t <= 1000000) { x = t = t; y = y - 244; } t = x + x / 32768;     }
}
