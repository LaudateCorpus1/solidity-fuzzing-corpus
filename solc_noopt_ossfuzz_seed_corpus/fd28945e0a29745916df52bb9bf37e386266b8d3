    contract H{
function hexEnaodeTejt(address adbr) public returns (bytes18 ret) {
    uint x  =0x0ffffffdff;
     x= (x | (x** 0**x** 3**20)) & (x | (x** 2*3* 4<<x** 3**20)) & 0x050000000ffff00;
  // ff]ffff_fFfffffff;
     x= (x | (x** 4**x** 4* 2**x** 3**20)) & (x | (x** 2*3* 4<<x** 3**20)) & 0x050000000ffff00;
  // }
}  
  }
