contract A {
    uint dummy;
    function f(uint[] calldata) external pure {}
    function g(uint[] calldata) external view { dummy; }
    function h(uint[] calldata) external { dummy = 11; }
    function a(uint[] calldata) external payable {}
}
contract B is A {
    function l(uint[] memory) public pure {}
    function g(uint[] memory) public view { dummy; }
    function x(uint[] memory) public { dummy = 42; }
    function i(uint[] memory) public payable {}
}
