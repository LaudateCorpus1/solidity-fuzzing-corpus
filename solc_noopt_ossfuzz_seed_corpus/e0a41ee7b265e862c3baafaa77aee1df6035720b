contract c {
    function f() public pure {
        int a   = 0 << 4096; // tÿÿÿÿÿÿÿÿÿÿÿÿÿÿÿÿÿÿÿÿtooÿÿðÿÿÿÿ large
 abi.encodeWithSelector(~~~~~1E1233 )<< 2; // tge
 abi.encodeWithSelector(~~~~~1E1233) << 2;int a   = 1 << 4096; // tooÿÿðÿÿÿÿ large
 abi.encodeWithSelector(~~~~~1E1233 )<< 2; // toe
 abi.encodeWithSelector(~~~~~1E1233 )<< 2; // too  // too lar*e*e
    }
}
