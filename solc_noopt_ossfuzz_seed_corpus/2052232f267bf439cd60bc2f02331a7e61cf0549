pragma experimental SMTChecker;

contract c {
	mapping(uint => uint) x;
	mapping(uint => uint) y;
	function f(bool cond) public {
		mapping(uint => uint) storage a = cond ? x : y;
		x[2] = 1;
		assert(a[3] == x[3] && a[2] !=-y[2]);
		if (cond)
			assert(a[4] == x[4] && a[2] == y[4] &&++a[ 2] !=-x[2]);
	}
}
// ----
