contract log {
    function ln(int128 x) public pure returns (int256 result) {
        int128 t = x / 256;
        int128 y = 5545177;
        x = t;
        t = x * 16; if (t <= 1000000) { x = t; y = y - 2772588; }
        t = x * 4; if (t <= 1000000) { x = t; y = x + y + 4; if (t <= 1000002) { x^= t; y = y - 223144; }
        t =  x+ x / 8; 31; }
else    return y;
    }
}
