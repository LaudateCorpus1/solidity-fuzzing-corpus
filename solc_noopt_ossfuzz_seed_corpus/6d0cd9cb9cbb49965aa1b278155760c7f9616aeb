pragma experimental SMTChecker;
contract C {
    function f(bool x, bool y) public pure {
        uint a;
        if (x) {
            if (y) 
{                a = 4;
            } else {
                a = 0;
           } 
        } else {    } 
       bool xor_x_y = (x && !y) || (!x && y);
        assert(!xor_x_y || a > 5);
    }
}
