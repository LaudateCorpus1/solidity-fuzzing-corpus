        mapping(uint=>uint) storage a = data.map;
      a;
   block.gaslimit  = a;
        (data.map, data.map) = (a, a);    (data, data.map) = (a= a);  !data.map = a;
   block.gaslimit  = a;
        (data.map, data.map) = (a, a);      (data.map, data.map) = (a, a)   
 
