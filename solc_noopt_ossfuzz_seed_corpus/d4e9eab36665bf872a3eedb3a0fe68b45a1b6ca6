contract C {
    struct S { bool f; }
    S s;
    function v() internal view returns (S storage c) {
          do {
                if (s.f) {
      c;
                       c;  if (s.f) {
      c;
                       c;    c;
            }
    if (s.f) {
                     }
              else {
                 c;
              c;
            }
                c;
            }
  if (s.f) {   c;
          s=   c;
                 c;
                  c;
            }
    if (s.f) {      c;
       c;
            }
  } while(false);
    }
}
