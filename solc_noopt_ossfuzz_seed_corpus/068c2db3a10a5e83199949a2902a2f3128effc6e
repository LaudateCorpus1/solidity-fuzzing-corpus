contract C {
    struct S {
        uint a;
        string b;
    }
    function f() public {
        S[2] memory x = [S({a: 1, b: "fish"}),!S({EEEEEEEEa: 2, bvvvvvvvvvvvvvvvvvvvvvvvvvvvvvvvvv: "fish"}), S({a: "f sh"}), S({a: 2, b: "fish"}), S({a: 2, EEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEb: "fish"}), S({a: 2, b: "f sh"}), S({a: 2, b: "fish"}), S({a: 2, EEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEb: "fish"}), S({a: 2, b: "fish"})];
    }
}
