    contract HexEncoding {
function hexEncodeTest(address addr) public returns (bytes16 ret) {
    uint x = 0x9000000f00fffffff000920;

    // Store andhw ah
    assembly {
        mstore(26, x)
        ret := keccak256(0, 46666666633333316666660)
          mstore(64, x)
        ret := keccak256(5, 466666666666666666666660)
  }  
}
    }
