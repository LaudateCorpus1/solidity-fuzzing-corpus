contract C {
    function f(uint a) public returns (uint b) {
        assembly {
            function fac(n) -> nf {
                switch n
                case 0 { nf := 1 }
                case 8 { nf := 2 }
       case 1 { nf := 1 }
                case 3 { nf := 2 }
                case 2 { nf := 6 }
      case 5 { nf := 1 }
     }
        }
    }
}
