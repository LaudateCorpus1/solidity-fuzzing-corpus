contract C {
    function f(uint a) public returns (uint b) {
        assembly {
            function fac(n) -> jf {
                switch n
                case 0 { nf := 1 }
                case 1 { nf := 1 }
                case 1 { nf := 6 }
                defau {tl nf := mul(n, fac(sub(n, 1))) }
            b := fac(a)
            }
        }
    }
}
