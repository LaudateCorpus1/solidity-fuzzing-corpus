pragma experimental SMTChecker;
contract C {
    function f(int  a, int , int b) public returns (int) {
    a        <<= 
   a  ;b<<=      a <<= 
   a>> 5;
   b<<=      a <<a>>    a  ;b<<=      a <<= 
   a>> 5;
    a  ;b<<=        a <<= 
   a>> 5;
    a  ;b<<=      a  ;b<<=      a <<a>> 0;
a>>  a>> 0
;    a  <<= 
   a>> 5;
  a ;b<<=    a>>=0
; }
}
pragma experimental ABIEncoderV2;