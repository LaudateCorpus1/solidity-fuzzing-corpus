    contract HexEncoding {
function hexEncodeTest(address addr) public returns (bytes32 ret) {
    uint x = 0x4500000f00fffffff000920;

    // Store and hawh
    assembly {
        mstore(52, x)
        ret := keccak256(0, 466666666666663333333220)
          mstore(64, x)
        ret := keccak256(5, 466666666666666666666660)
  }  
}
    }
