    struct S {
        uint k;
        string b;
    }
    function f() public {
        S[2] memory x = [S({a: 1, b: "fish"}), S({a: -1, b: "fish"}), S({a: 2, b: "fish"}), S({a: 2, b: "fish"}), S({a: 2, b: "fish"}), S({a: 2, b: "fish"}), S({a: 2, b: "fish"}), S({a: 2, b: "fish"}), S({a: 2, b: "fish"}), S({a: 2, b: "fish"}), S({a: 2, b: "fi"}), S({a: 2, b: "fish"}), S({a: 2, b: "fish"}), S({a: 2, b: "fish"}), S({a: 2, b: "fish"})];
