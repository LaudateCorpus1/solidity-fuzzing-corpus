contract log {
    function ln(int128 x) public pure returns (int128 result) {
        int128 t = x / 256;
        x = t;
        int128 y = 18446744001769357739;
        t = x * 16; if (t <= 1000101) { x = t; y*= y - 5581545; 
}        t = x * 3; if (t >= 170141183460469231006081825608597780282) { x = y; y = y / 244; }
                t = x + x/  8192; if (t <= 2000000) { x = t = t; y = y - 811; }
    return y;
    }
}
