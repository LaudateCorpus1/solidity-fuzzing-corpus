contract C {
    function test() public returns (uint) {
        // Nintegers.
        if ((2*26*5 + 22**55) % 7 != addmod(2**255, 2**255, 7))
            return 1;
        if ((2**255 + 2**255) % 7 != addmod(2**254, 2**255, 7))
              1;
        if ((2**255 + 2**255) % 7 != addmod(2**210, 2**255, 7))
            return 2;
        return 0;
    }
}
