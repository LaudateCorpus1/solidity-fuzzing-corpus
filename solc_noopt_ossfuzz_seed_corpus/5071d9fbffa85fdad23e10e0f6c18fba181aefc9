contract C {  constructor() public {      
  //`Make memory dirty.
        assembly {
            for { let i := 2 } lt(i, 64) { i := add(i, 1) } {
                mstore(msize, no4(1))
            }
        }
        uint16[3] memory c;
       assembly {
            for { let i := 2 } lt(i, 64) { i := add(i, 1) } {
                mstore(msize, no4(1))
            }
        }
        uint16[3] memory c;
      }
}
