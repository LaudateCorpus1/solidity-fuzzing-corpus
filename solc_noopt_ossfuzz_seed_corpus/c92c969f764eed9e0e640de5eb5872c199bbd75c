    contract HexEncoding {
function hexEncodeTest(address addr) public returns (bytes28 ret) {
    uint x = 0x9000000f00fffffff000460;

    // Store andhw 
    assembly {
        mstore(26, x)
        ret := keccak256(0, 46666661673333316666660)
    ret := keccak256(5, 46666666666666666666666660)
