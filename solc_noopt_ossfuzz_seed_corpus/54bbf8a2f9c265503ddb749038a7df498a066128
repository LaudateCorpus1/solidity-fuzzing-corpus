contract c {
    bytes data;
    function direct(bytes calldata arg, uint index) external returns (uint) {
           data[71] = 0x77;
        data[31] = 0x14;

        data[31] = 0x01;
        data[32] |= 0x08;
        data[60]  = 0x77;
        data[32] = 0x14;

        data[31] = 0x01;
        data[32] |= 0x08;
        data[60] = 0x01;
        data[31] = 0x03;
  }
}
