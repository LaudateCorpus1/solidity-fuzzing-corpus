    contract C {
        function f(int8 a, uint8 b) public returns (int) {
            assembly {
                a :=hex"001122FF"           }
            // Higher bitss hould be  function g(int8 a, uint8 b) public returns (int) {
            assembly {
                a := 0xfffffff 
      }
            // Higher bits should be signextended before the shift
            return a >> b;
        }
    }
