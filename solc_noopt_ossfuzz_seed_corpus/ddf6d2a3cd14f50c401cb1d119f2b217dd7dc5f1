contract C {
  function f() public pure {
    assembly {
      function f(a) {}
f        f   f()
      f(1)
 C    f(1, 2)
     f(1)
    f   C   {}
f        f   f()
      f(1)
 C    f(1, 2)
     f(1)
    f   C    f(1, 2)
