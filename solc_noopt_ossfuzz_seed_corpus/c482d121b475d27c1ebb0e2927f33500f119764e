contract A {
	uint public x;
}
contract C is A {
	function x() public returns (uint);
}
// ----pragma expercontract C { constructor(uint) public {} }
contract A is C { constructor() C(2) public {} }
contract B is C {	uint public x;
}
contract C is A {
	function x() public returns (uint);
}
// ----pragma expercontract C { constructor(uint) public {} }
contract A is C { constructor() C(2) public {} }
contract B is C { constructor() C(2) public {} }
contract D is A, B { }
// --- constructor() C(2) public {} }
contract D is A, B { }
