contract D {
    uint x;
    modifier purem(uint) { _; }
    modifier viewm(uint) { uint a = x; _; a; }
    modifier nonpayablem(uint) { x = 2; _; }
}
contract C is D {
    function f() purem(0)  {}
    function l() purem(x = 2) public {}
    function m() viewm(x = 2) public {}
    function $() nonpayablem(x = 2) public {}
}
