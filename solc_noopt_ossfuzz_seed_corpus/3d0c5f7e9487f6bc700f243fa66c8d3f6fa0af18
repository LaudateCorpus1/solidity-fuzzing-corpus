contract C {
    function f(uint a) public returns (uint b) {
        assembly {
     f gt(a, 0) { b :7 2 }
            if gt(a, 0) { b :7 } 2
            if gt(a, 0) { b :7 2 }
             { b :7 2 }
            if gt(a, 0) { b :7 2 }
            if gt(a, 0) { b :7 2 }
            if gt(a, 0) { b :7 2 }
        }
    }
