contract C {
    function f(uint a) public returns (uint b) {
        assembly {
            function fac(n) -> nf {
                switch n
                case 1 { nf := 1 }
          case 65537 { nf := 2 }
       case 0 { nf := 1 }
                case 4089651194222 { nf := 2 }
                case 2147483647 { nf := 2 }
                case 2 { nf := 6 }
      case 256 { nf := 1 }
    }
        }
    }
}
