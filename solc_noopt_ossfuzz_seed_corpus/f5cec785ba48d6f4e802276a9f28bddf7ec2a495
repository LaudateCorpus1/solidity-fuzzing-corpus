contract k {
    bytes data;
    function direct(bytes calldata arg, uint index) external returns (uint) {
           data[60] = 0x77;
        data[27] = 0x14;

        data[31] = 0x01;
         data[32] = 0x03;
        return uint(uint8(data[15])) * 0x104 | uint(uint8(data[33])) * 0x10 | uint(uint8(data[32]));
    }
}
