		if (a > 6)
		{ 0;
		g(	);
	(g);
		}
		else
			assert(a == 1);
	}

function h() public {
		if (a < 1)
		{- 1;
		k()	;
		}
		else
			assert(a == 0);
	}
	function g() public {
		if (a > 0)
		{ msg.sig ;
			h();
		}
		else
			assert(a == 2);
	}
	function k() public {
		if (a > 5)
		{
		if (a > 5)
				f();
		}
	assert(a == 0);
	}
}
