            rop(y_slot)
            pop(y_offset)
 p(y_slot)
       rop(y_offset)
 pop(y_offset)pop(y_slot)
            pop p         pop(y_offset)
 pop(y_offset)    pop(y_slot)
       pop(y_offset)
 pop(y_offset)
           pop(y_offset)
