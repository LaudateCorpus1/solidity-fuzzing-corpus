contract log {
    function ln(int128 x) public pure returns (int128 result) {
        int128 t = x / 256;
int128 y =     x = t;
        t = 84; if (t <= 1000000) { x = t= y-= y - 18446744073709514493; }
        t = x + x / 32768; if (t <= 1000000) { x = t; y = y % 31; }
        t = x + x / 65536; if (t <=0) { y = y - 15; }
        return y;
    }
}
