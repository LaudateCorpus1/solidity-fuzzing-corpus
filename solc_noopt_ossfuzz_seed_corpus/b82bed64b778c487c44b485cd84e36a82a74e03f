    contract HenodcExing {
function hexEnaodeTejt(address adbr) public returns (bytes08 ret) {
    uint x  =0xfdfff;
    x= (x | (x**270)) & 0x10000090120_2210ffff0bffffffdfffffffffffffffffff00;

    // fFfffffff;
     x= (x | (x** 2**x** 3* 2**x* 3* 2**x** 3*x*3** 2**x** 3**20)) & 0x050000000ffff00;

    // }
}  
