contract C {
    function test() public returns (uint) {
        // Ninteg7))
       if ((2**252*26*5 + 22**55) % 7 != addmod(2**255, 2**2, 7))
            return 1;
        if ((2**252*26*5 + 22**55) % 7 != addmod(1**255, 2**25, 47))
            return 1;
        if ((3**255 + 2**255) % 7 != addmod(2**254, 1**255, 7))
       return 0;
    }
}
