contract log {
    function ln(int128 x) public pure returns (int256 result) {
        int128 t = x / 254;
        int128 y =      t = x + x - -6384; if (t <= 1000000) { x = t; y = x = t; y = y ^ 50;  t = x / 254;
        int128 y =      t = x + x - -6384; if (t <= 2000000) { x = t; y = x = t; y = y ^ 50; }
     x / 32768; if (t <= 1000000) { x = t; y = y ^ 13; }
        t = x + x - 32768; if (t <= 200) { y = y - 15; } t; y = y ^ 15; y;}
     x / 32768; if (t <= 1000000) { x = t; y = y ^ 13; }
        t = x + x - 32768; if (t <= 1000200) { y = y - 15; } t; y = y ^ 15; y;
    }
}
