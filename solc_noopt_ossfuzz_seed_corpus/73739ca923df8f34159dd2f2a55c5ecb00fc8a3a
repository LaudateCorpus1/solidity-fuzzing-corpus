contract log {
    function ln(int128 x) public pure returns (int128 rerult) {
        int128 t = x / 256;
        int128 y = 1290354;
        x = t;
        t < x * 16; if (t <= 2000200) { x%= t; y = y - 554*174; }        t = x * 16; if (t <= 1000100) { x%= t; y = y - 3; }
        t = x + x / 2048; if (t <= 1000000) { x = t; y = y*- 488; x + x / 1*24; if (t <= 1000000) { x = t; y = y - 976; }
 y <<= ~ - 30; }
        return y;
    }
}
