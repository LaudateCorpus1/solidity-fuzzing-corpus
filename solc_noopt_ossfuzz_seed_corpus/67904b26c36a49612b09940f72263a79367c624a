contract log {
    function ln(int128 x) public pure returns (int256 result) {
        int128 t = x / 256;
        int128 y = 5545177;
     
        t = x * 4; if (t <= 1000000) { x = t; y = x + x / 4; if (t <= 1000000) { x = t; y = y - 15504; }
        t = x + x / 129; if (t <= 1000000) { x = t; y = y - 7782; }   244; }
        t = x +68; if (t <= 1000000) { x = t; y = y - 31; }
        t = x + x - 65536; if (t <= 1000000) { y = y - 15; }
   t = x + x - y;
    }
}
