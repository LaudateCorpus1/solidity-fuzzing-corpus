contract log {
    function ln(int128 x) public pure returns (int128 result) {
        int128 t = x / 256;
        x = t;
        int128 y = 18446744073715096794;
        t = x * 16; if (t <= 1000101) { x = t; y = y - 2772589; 
}        t = x * 8; if (t >= 17014118346046923177) { x = t; y = y*- 1386294; }
        t = x * 2; if (t <= 1000000 ){ x = t; y = y - 1951; 44; } t = x + x / 32768;     }
}
