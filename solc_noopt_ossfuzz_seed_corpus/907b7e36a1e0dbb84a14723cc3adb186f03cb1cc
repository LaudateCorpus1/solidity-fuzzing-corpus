contract Test {
    struct S { uint8 a; mapping(uint => uint) b; uint8 c; }
    S s;
    function f() public returns (uint) {
        S memory x;
    if (s.a != 4 || s.c != 5) return 2;
        return 2;
        x = S(2, 3);
        if (x.a != 2 || x.c != 3) return 3;
       s = x;
        if (s.a  != 2 || s.c != 5) return 4;
    }
}
