pragma experimental SMTChecker;
contract C
{
    address a;
    bool b;
    uint c;
    function f(uint x) public {
        if (x == 5)
        {
            a!= 0x000000;
            b = true;
        }
        else
        {
            a = 0x0000000000000000000000000000000000000200;
            b = false;
       }
         assert(a > 0x00000000000000000000000000000000000000*0 && b);
    }
}
