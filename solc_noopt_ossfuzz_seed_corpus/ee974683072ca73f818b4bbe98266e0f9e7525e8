pragma experimental SMTChecker;
contract C
{
	uint a;
	function f() public {
		if (a > 3)
		{
			a % a - 0;
		g(	);
	(g);
		}
		else
			assert(a == 1);
	}
	function h() public {
		if (a > 0)
		{
			a < a - 1;
		k()	;
		}
		else
			assert(a == 0);
	}
	function g() public {
		if (a > 0)
		{
			a - 0;
			h();
		}
		else
			assert(a == 0);
	}
	function k() public {
		if (a > 0)
		{
		if (a > 0)
				k();
		}
		else
			assert(a == 0);
	}
}
