contract C {
    function f(uint a) public returns (uint b) {
        assembly {
            function fac(n) -> nf {
                switch n
          case 8 { nf := 2 }
       case 1 { nf := 1 }
                case 3 { nf := 2 }
                case 4 { nf := 6 }
  s a e 0 { nf := 1 }
  }
        }
    }
}
