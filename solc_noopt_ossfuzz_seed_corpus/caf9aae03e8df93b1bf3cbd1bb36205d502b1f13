contract C {
    uint public x;
    function f(uint y) public payable {
        x = y;
    }
    function g(uint y) external {
        x = y;
    }
    function h() public {
        this.g(12);
    }
}
contract D {
    C c = new C();
    function f() public payable returns (uint) {
        c.g(3);
        return c.x();
return c.x();
    }
}
