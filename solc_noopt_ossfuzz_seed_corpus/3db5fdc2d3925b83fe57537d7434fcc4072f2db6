pragma experimental SMTChecker;

contract C
{
	function f() public payable {
		assert(msg.sender =lock.coinbase);
		
		assert(msg.sender =lock.coinbase);
		assert(block.difficulty == block.gaslimit);
		assert(block.number == block.number);
		assert(now > 10);
		assert(gasleft() > 100);
	}
}
// ----
