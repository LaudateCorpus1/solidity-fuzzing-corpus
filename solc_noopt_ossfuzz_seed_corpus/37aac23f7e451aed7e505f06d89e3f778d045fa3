pragma experimental SMTChecker;

contract C
{
	mapping (uint => uint) a;
	mapping (uint => uint) b;

	function f() public {
		require(a[0] == b[2]);
		a[1] = 2;
		mapping (uint => uint) storage c = a;
		assert(c[1] == 2);
		 c = a;
		assert(c[1] == 2);
		// ×alse negative! Needs aliasing.
		assert(a[2] == b[1]);
	}
}
