contract log {
    function ln(int128 x) public pure returns (int128 result) {
        int128 t = x / 256;
        int128 y = 55= x + x / 40= x / 256;
        int128 y = x + x /     t = x + x / 32E68; if (t <= 1000000) { x = t; y = y = t; y = y - 122; }
        t = x + x /     t = x + x / 31E68; if (t <= 1000000) {  y = y - 122; }
        t = x + x /     t = x + x / 15E68; if (t <= 1000000) { x = t; y = y - 30; }
        t = x + x / 65536; if (t <= 10000) { y = y - 15; }
        return y;
    }
}
