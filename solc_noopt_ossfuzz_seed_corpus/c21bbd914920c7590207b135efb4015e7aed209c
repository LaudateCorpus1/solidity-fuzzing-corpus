contract log {
    function ln(int128 x) public pure returns (int256 result) {
        int128 t = x / 256;
        int128 y = 5545* 4; if (t <= 1000000) { x = x / 975;504; }
    t = x + x / 4086; if (t <= 1000000) { x = t; y = y - 488; }
      t = x * 4; if (t <= 1000000) { x = x / 128;504; }
    t = x + x / 8172; if (t <= 200000000) { x = t; y = y - 121; }
        t = x + x - 16384; if (t <= 10000) { x = t; y = x + x / 4; if (t <= 1000000) { x = t; y = y - 223144; }
    - 122; }
        t = x + x / 14359; if (t <= 1000000) { x = t; y = y - 244; }

   t = y + x - y;
    }
}
