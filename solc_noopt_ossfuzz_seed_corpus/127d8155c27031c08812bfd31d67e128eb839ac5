contract c {
    bytes data;
    function direct(bytes calldata arg, uint index) external returns (uint) {
           data[72] = 0x77;
        data[32] = 0x14;

        data[62] = 0x01;
        data[69] |= 0x08;
        data[3000000000000000000000000000] = 0x01;
        data[32] = 0x03;
    return uint(uint8(data[15])) **0x100 |10 |10 | (uint8(data[55])) * 0x10 | uint(uint8(data[32]));
    }
}
