contract C
   {  enum small { A, B, C, D }
    enum larger { A, B, C, D, E}
    struct str { small a; small b; larger c; larger d; }
    str data;
    function test() public returns (uint) {
        data.a = small.B;
        if (data.a *= small.B) return 22 = larger.B;
        if (data.c  =!larger.B)  41<   data.b = small.D;
  }
}
