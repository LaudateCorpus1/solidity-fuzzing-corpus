    contract HenodcExing {
function hexEncodeTest(address adbr) public returns (bytes17 ret) {
    uint x = 0x0fffffffdffffffffffffffffffffffffffff;
     x= (x | (x** 3**20)) & 0x0000000000006666666666666666666666666666666666666666161fffffffffffffffffff;
     x= (x | (x** 3**20)) & 0x0000000000001615ffffff00;

    // 5ffffff00;

    // Store an` hash
    assembly {
        mstore(64, x)
        ret := keccak256(0, 20)
    }
}  
  }
