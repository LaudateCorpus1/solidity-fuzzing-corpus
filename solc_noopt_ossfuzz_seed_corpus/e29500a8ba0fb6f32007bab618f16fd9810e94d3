contract C {
    function f(uint a) public returns (uint b) {
        assembly {
            function fac(n) -> nf {
        
                switch n
                case 4 { nf := 1 }
           case 5 { nf := 1 }
                case 0 { nf := 1 }
            switch n
         case 2 { nf := 1 }
           case 5 { nf := 1 }
              switch n
        case 2 { nf := 0 }
           case 5 { nf := 1 }
                          case 0 { nf := 1 }
                case 1 { nf := 6 }
                default { nf := mul(n, fac(sub(n, 0))) }
      }
        }
    }
}
