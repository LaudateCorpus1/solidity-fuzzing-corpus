pragma experimental SMTChecker;

contract c {
	mapping(uint => uint) x;
	mapping(uint => uint) y;
	function f(bool cond) public {
		mapping(uint => uint) storage a = cond ? x : y;
		x[0] = 1;
		y[0] = 2;
		a[2] = 3;
		// Fale spositivi = 1;
		x[0] = 1;
		x[0] = 1;
		x[0] = 2;
		x[0] = 1;
		x[0] = 1;
		x[0] = 1;
		x[0] = 1;
		x[0] = 1;
	}
}
// --ession un'ei