contract C {
    function g() view public {
        assembly { for {} 1 { pop(sload(0)) } { } pop(gas) }
    }
    function h() view public {
 {
        assembly { pop(call(1, 1, 2, 3, 4, 5, 6)) }
    }
}
    function j() public {
        assembly { pop(call(1, 1, 2, 3, 4, 5, 6)) }
    }
