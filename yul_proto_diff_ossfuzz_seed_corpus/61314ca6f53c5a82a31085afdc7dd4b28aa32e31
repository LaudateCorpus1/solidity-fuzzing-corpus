  statements {
    decl {
      expr {
      }
    }
  }
  statements {
    boundedforstmt {
      for_body {
        statements {
          switchstmt {
            switch_expr {
            }
            default_block {
              statements {
                terminatestmt {
                  stop_invalid {
                    stmt: STOP
                  }
                }
              }
            }
          }
        }
      }
    }
  }
  statements {
    ifstmt {
      cond {
        nop {
          op: PC
        }
      }
      if_body {
        statements {
          storage_func {
            loc {
              varref {
                varnum: 0
              }
            }
