
pragma solidity >=0.0;



contract C0 {

	function f0() public pure
	
	returns (uint)
	{
		return 48271;
	}
	function f1() public pure
	
	returns (uint)
	{
		return 182605794;
	}
}

contract C1 {

	function f0() public view
	
	returns (uint)
	{
		return 1291394886;
	}
}


contract C {
	function test() public returns (uint)
	{
			C0 tc0 = new C0();
			if (tc0.f0() != 48271)
				return 1;
			if (tc0.f1() != 182605794)
				return 2;
			C1 tc1 = new C1();
			if (tc1.f0() != 1291394886)
				return 3;
			return 0;
	}
}

