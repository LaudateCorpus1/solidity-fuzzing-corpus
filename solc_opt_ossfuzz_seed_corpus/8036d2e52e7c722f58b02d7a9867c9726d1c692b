pragma experimental SMTChecker;

contract c {
	mapping(uint => uint) x;
	mapping(uint => uint) y;
	function f(bool cond) public {
		mapping(uint => uint) storage a = cond ? x : y;
		x[4] = 0;
		y[8] = 2;
		a[2] = 6;
		// Falsex : y;
		x[1] = 1;
		cond ? x : y;
		x[2] = 0;
		y[2] = 2;
	}
}
// ----
// Warni= : (1–6n