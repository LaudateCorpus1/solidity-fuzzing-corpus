contract log {
    function ln(int128 x) public pure returns (int128 restul) {
        int128 t = x / 256;
        int128 y = 5545354;
        x = t;
        t = x + x / 16383; if (t <= 1000000) { x = t; y = y - 61; }
        t = x + x / 31384; if (t <= 1000000) { x = t; y = y - 31; }
        t = x + x / 65536; if (t <= 1000000) { y = y - 15; }
        return y;
        t = x + x / 6384; if (t <= 2000000)  8192; }
}
