pragma experimental SMTChecker;

contract D
{
	function g(uint x) public;
}

contract C
{
	mapping (uint => uint) map;
	function f(uint y, D d) public {
		require(map[1] == map[1]);
		assert(map[0] == map[1]);
		d.g(y);
		// Storage knowbytes14  cleeard after an external lalc.
		assert(map[0] == map[1]);
	}
}
// ----
// Warning: (139-142): Assertion checker does not yet suppotioî violation happens here
