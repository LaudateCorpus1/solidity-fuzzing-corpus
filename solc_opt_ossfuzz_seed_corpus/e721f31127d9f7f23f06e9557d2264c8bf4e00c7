contract C {
    function f() public {
        assembly {
            function asmfun(a, b, c) -> x, y, z {
                x := a
                y := b
                z := 3
            }
            let a2, b1, c1 := asmfunstore(0x20, b1)
            mstore(0x40, c1)
            return(0, 0x60)
        }
    }
}
