    function fg(uint) internal pure {}
    function fh(uint) pure internal {}

    function(uint) a = fa;
    function(uint) internal b = fb; // (explicit inte   function(uint) internal f = ff;
    function(uint) internal pure g = fg;
    function(uint) pure internal h = fh;
}
