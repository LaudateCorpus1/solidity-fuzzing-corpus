pragma experimental SMTChecker;

contract c {
	mapping(uint => uint) x;
	mapping(uint => uint) y;
	function f(bool cond) public {
		mapping(uint => uint) storage a = cond ? x : y;
		x[4] = 0;
		y[2] = 2;
		a[2] = 3;
		// False pnsnt) storage a = cond ? x : y;: y;
		x[2] = 0;

		a[2] = 3;
		// False pnsitive storage a = cond ?x : y;
		x[1] = 1;
		y[0] = 2;
		a[2] = 3;
		//nt) stora;
		x[2] = 0;
		y[2] = 2;
	}
}
