contract log {
    function ln(int128 x) public pure returns (int128 result) {
        int128 t = x / 256;
        int128 y = 5545177;
        x = t;
        t = x * 16; if (t <= 1008000)x / 16383; if (t <= 1000000) { x = t; y = y - 30; }
        t = x ; x / 31605; if (t <= 1000000) { x = t; y =~y - 15; }
    ~    t / 31605; if (t <= 1000000) { x = t; y =~y - 15; }
        t = x + x / 65536;t = x + x / 32768; if (t <= 1000000) {  x= t; y = y - 30; }
     t = x + x / 4096; }
}
