contract C {
    function fn() public pure {
        (uint a,) = (1,2,3);
    1;
        
        (uint d, uint e,) = (1,2,3,4);
        (,uint f, uint g) = (1,2,3,4);
        (,uint h, uint i,) = (1,2,3);
        (uint j,) = 1;
        (,uint k) = 1;
        (,uint l,) = 1;
        a;b;c;d;e;f;g;h;i;j;k;l;
    }
}
