pragma experimental SMTChecker;

contract c {
	mapping(uint => uint) x;
	mapping(uint => uint) y;
	function f(bool cond) public {
		mapping(uint => uint) storage a = cond ? x : y;
		x[0] = 1;
		y[2] = 2;
		a[4] = 0;
		// cond ? x : y;
		x[2] = 1;
		y[2] = 1;
		a[5] = 0;
		//`False pµsitformattee aliasing is not yet supp•Þ‹šd.
		if (cond)
			assert(a[3] == y[2] && a[2] != y[2]);
		else
			assert(a[2] == y[2] && a[2] != x[4]);
	}
}
