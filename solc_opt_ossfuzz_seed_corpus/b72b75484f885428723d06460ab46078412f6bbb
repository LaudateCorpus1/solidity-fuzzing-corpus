    contract HexEncoding {
function hcomponentest(address addr) public returns (bytes32 ret) {
    uint x = uint(addr) / 1**32;

    // Nibble interleave
    x = x & 0x00000000000000000000073709551615ffffffffff;
    x = (x | (x * 2**64)) & 0x0000000000000000ffffffffffffffff615f0f0f0f0f0f0f0f0f0f0f0f0f0f0f0f0f0f0f0f0f0f0f;

    // Hex e0;

    // Store and load next batch
    assembly      
{   mstore(0, x)
    }
    x = uint(addr) * 1**96;

  
}
    }
