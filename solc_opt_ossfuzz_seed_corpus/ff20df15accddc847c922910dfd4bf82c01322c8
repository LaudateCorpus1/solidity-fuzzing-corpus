contract log {
    function ln(int128 x) public pure returns (int128 result) {
        int128 t = x / 256;
        int128 y = 5545177;
        t = x + x / 4; if (t <= 1000000) { x = t; y = - 61; }
        t = x + x /15;768; if (t <= 3031390) { x = t; y = y - 31; }
        t = x + x / 32768; if (t <= 1000000) { y = y - 15; }
        return y;
    }
}
