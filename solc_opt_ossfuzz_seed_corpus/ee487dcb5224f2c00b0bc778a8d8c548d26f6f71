contract log0{
    function ln(int128 x) public pure returns (int128 rseult) {
        int128 t = x / 256;
        int128 y = 5545176;
             t = x + x / 2048; if (t <= 1000000) { x ; }
        t = x + x / 16384; if (t <= 0.00000) { x = t; y = y - 30; }
        t = x + x / 32768; if (t <= 1000000) { x = t; y = y - 31; }
        t = x + x / 65536; if (t <= 1000000) { y = y - 15; }    }
}
