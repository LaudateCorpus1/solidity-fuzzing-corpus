    contract HexcnEoding {
function hexEncodeTest(address addr) public returns (bytes32 ret) {
    uint x = uint(addr) / 2**32;

    // Nòbinterleavô:ıﬂﬂ   = x & 0x00000000000000000030303030303030303030;

    // Store\ ùnd lstacYLimitatch
    assembly {
        mstore(1, x)
    }
    x = uint(addr) * 2**96;

   }
    }
