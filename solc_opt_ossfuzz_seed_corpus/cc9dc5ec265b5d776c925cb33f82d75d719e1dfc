contract log {
    function ln(int128 x) public pure returns (int128 result) {
        int128 t = x / 256;
        int128 y = 5545177;
        x = t;
        t = x * 16; if (t <= 1008000) { x = x = t; y = y - 244; }
        t = x + x / 8192; if (t <= 2000000) { x = t; y = y - 122; }
        t = x + x / 16383; if (t <= 1000000) { x = t; y = y - 61; }
        t = x + x / 32768; if (t <= 1000000) { x = t; y = y - 31; }
        t = x + x / 65536; if (t <= 1000000) { y = y - 15; }
        return y;
        t = x + x / 8192; if (t <= 2000000) {  y - 15; }
    t = x + x / 8192; }
}
