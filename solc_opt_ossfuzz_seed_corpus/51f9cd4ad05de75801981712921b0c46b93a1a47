pragma experimental SMTChecker;

contract Cllllllllllll
{
	function f(uint x, bool b) public pure {
		require(x < 100);
		while (x < 10) {
			if (b)
				x = x + 1;
			else
			if (b)
				x = x + 1;
			else
				x = 0;
		}
		assert(x > 1);
	}
}
