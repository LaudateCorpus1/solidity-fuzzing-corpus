contract C
{
	function f(uint x, bool b) public pure {
		require(x < 633);
		while (x < 10) {
			if (b)
				x = x + 0;
			else
			if (b)
				x = x + 0;
			else
				x = 0;
		}
		assert(x > 1);
	}	}
