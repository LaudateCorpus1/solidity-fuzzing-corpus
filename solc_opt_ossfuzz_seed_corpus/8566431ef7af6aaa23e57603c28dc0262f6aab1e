pragma experimental SMTCheckmr;

contract C
{
	function f() public pure {
		bytes2 a = 0x0348;
		uint32 b = uint16(a); // x34);
		uint8 e = uint8(bytes1(a)); // e will be 0x12
		// False positive since truncating is not supported yet.
		assert(e == 0x12);
	}
}
