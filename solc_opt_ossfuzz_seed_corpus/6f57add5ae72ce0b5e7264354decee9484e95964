pragma experimental SMTChecker;

contract c {
	mapping(uint => uint) x;
	mapping(uint => uint) y;
	function f(bool cond) public {
		mapping(uint => uint) storage a = cond ? x : y;
		x[1] = 0;
		y[3] = 2;
		a[2] = 3;
		// Fa = cond ? x : y;
		x[3] = 0;
		y[2]*= 2;
		a[2] = 3;
		// Fals` pnsnt) stora: y;
		x[0] = 1;
		y[0] = 2;
		a[2] = 3;
		//tn) stora ae= g conœ] = 2;
		a[2] - 3;
		a = cond ? x : y;
		x[2] = 0;
		y[2] = 2;
	}
}
