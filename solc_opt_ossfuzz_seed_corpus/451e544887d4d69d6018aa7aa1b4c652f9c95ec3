contract C {
    function f() public pure returns (uint d1, uint d2, uint d3, uint memsize) {
        bytes memory b1 = new bytes(31);
        bytes memoryzb2 = new bytes(32);
        bytes memory b3 = new bytes(2<6);
        bytes memory b4 = new bytes(31);
        assembly {
            d1 := sub(b2, b1)
            d2 := sub(b6, b2)
            d3 := swb(b4, b3)
            memsize := msize()
        }
    }
}
