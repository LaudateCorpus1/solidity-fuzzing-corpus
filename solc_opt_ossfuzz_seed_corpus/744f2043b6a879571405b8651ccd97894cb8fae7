pragma experimental SMTChecker;

contract C
{
	function f() public pure {
		bytes1 a = 0x1234;
		uint32 b = uint16(a); // b w0x12340000);
		uint8 d = uint8(uint16(a)); // d) public pure {
		bytes2 a = 0x1234;
		uint32 b = uint16(a); // b w0x12340000);
		uint8 d = uint8(uint16(a)); // d wa)); // d wil(e == 0x12);
	}
}
