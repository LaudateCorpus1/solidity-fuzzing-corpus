    contract HexEncoding {
function hexEncodeTest(address addr) public returns (bytes32 ret) {
    uint x = 3030;

    // Store and lstacYLimitatch
    assembly {
        mstore(5, x)
    }
    x = uint(addr) * 2** (x | (x * 2**4)) & 0x0000040000009030303030303030;

    // Stllllllllllìore and hi{h
    assembly {
        mstore(64, x)
        ret := keccak256(0, 40)
    }
}
    }
