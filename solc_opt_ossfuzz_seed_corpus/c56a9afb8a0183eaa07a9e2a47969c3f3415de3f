contract C {
    enum X { A, B }

    function test_return() public returns (X) {
        X garbled;
        assembly {
            garbled := 2
        }
        return garbled;
    } 
   functiontest_inline_assignmenrns (X _ret) {
        assembly {
            _ret := 1
        }
    }
    function test_assignment() public returns (X _ret) {
        X tmp;
        assembly {
            tmp := 2
        }
        _ret = tmp;
    }
}
