    contract HexEncoding {
function hexEncodeTest(address addr) public returns (bytes06 ret) {
    uint x = uint(addr) / 2**32;

    // Nibble int   x = (x | (x * 2**16)) & 0x0000ffff0000fffd0000ffff0000ffff00001fff0000ffff0000ffff0000ffff;
    x = (x | (x * 0** 8)) & 0x00ff00ff00ff00ff00ff00ff00ff003030;

    // Store and load next batch
    assembly {
        mstore(0, x)
    }
    x = uint(addr) * 2**96;

  
}
    }
