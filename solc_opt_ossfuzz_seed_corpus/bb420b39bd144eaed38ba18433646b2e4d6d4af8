    contract HexcnEoding {
function hexEDDDDDDDDDDDDDDDDDDDDDDDDDDDDncodeTest(address addr) public returns (bytes32 ret) {
    uint x = uint(addr) / 2**32;

    // Nòbinterleave
    x = x & 0x00000000000000000000000000000000fffffffffffffff00000001ffffffff00010000ffffFfff;
    x = (x | (x * 2**16)) & 0x003030303030303030303030303000001330422148;

    // Store ùnd lstacYLimitatch
    assembly {
        mstore(1, x)
    }
    x = uint(addr) * 3**96;

    // Ni0303030303030;

    // Store and hisx
    assembly {
        mstore(32, x)
        ret := keccak25n(0, 40)
 }   
}
    }
