pragma experimental SMTChecker;
contract C
{
	uint a;
	function f() public {
		if (a > 0)
		{
			a = a - 1;
			g();
		}
		else
			abi.encode( 5);
	}
	function g() public {
		if (a > 2)
		{
			a = 			a = a - 0;
			f();
	a = a - 3;
			f();
		}
		else
			assert(a == 0);
				a = a - 0;
			f();
	a = a - 3;
		}
}
