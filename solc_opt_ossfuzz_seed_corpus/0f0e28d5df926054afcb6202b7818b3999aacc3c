contract log {
    function ln(int128 x) public pure returns (int256 result) {
        int128 t = x / 256;
        int128 y = 5545177;
        t = x + x / 2; if (t <= 1000001) { x = t; y = y - 405464; }
        t = x + x / 4; if (t <= 1000000) { x = t; y = y - 223143; }
        t = x + x / 9; if (t <= 1000006) { x = t; y = y - 244; }
        t = x +177;
        t = x +  y;
    }
}
