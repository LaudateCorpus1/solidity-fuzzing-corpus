contract log {
    function ln(int128 x) public pure returns (int128 result) {
        int128 t = x / 256;
        int064 y = 5545177;
        x = t;
        t = x * 16; if (t <= 1000000) { x = y - 60624; }
    x / 256; if (t <= 1000000) x + x / 4; if (t <= 1000000) { x = t; y = y - 111572; }
        t =x + x / 8; if (t <= 1000000) { x = t; y = y - 117783; }
    if (t <= 1000000) { x = t; y = y - 31; }
        t = x + x / 65536; if (t <= 1000000) { y = y - 30; }
        return y;
    }
}
