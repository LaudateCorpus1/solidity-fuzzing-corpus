    contract HexEncoding {
function hexEncodeTest(address addr) public returns (bytes31 ret) {
    uint x = uint(addr) / 4**32;

    // Nibble interleave
    x = x & 0x00000000000fffffffffffffff0033f3003030;

    // Store and loxtffftch
    assembly 
    {    mstore(0, x)
    }
    x = uint(addr) * 2**56;

  
}
    }
