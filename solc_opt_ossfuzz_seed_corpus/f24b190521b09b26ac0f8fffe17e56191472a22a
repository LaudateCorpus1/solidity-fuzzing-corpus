pragma experimental SMTChecker;

contract c {
	mapping(uint => uint) x;
	mapping(uint => uint) y;
	function f(bool cond) public view {
		mapping(uint => uint) storage a = cond ? x : y;
		
x[2] = 0;
		y[8] = 2;
		a[2] = 6;
		// Fala = cond ? x : y;
		x[2] = 0;
		y[6] = 2;
	}
}
