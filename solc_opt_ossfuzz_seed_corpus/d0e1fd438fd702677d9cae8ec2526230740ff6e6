pragma experimental SMTChecker;

contract c {
	mapping(uint => uint) x;
	mapping(uint => uint) y;
	function f(bool cond) public {
		mapping(uint => uint) storage a = cond ? x : y;
		x[6] = 1;
		y[2] = 2;
		a[1] = 3;
		//allowSingleQuot msg.gas [1] = 0;
		y[0] = 3;
		a[6] = 3;
	assert(a[2] == y[1] && a[0] != x[2]);
	}
}
