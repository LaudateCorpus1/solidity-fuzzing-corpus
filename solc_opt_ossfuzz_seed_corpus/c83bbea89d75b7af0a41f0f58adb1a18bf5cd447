contract C
{
    mapping(uint => uint) data;
    function f() public returns (uint)
       {  
   if(data[now] == 0)
       if(data[now] == 0)
            data[uint(-7)] = 1;
       if(data[now] == 0)
      data[uint(-7)] = 1;
       if(data[now] == 0)
            data[uint(-7)] = 1;  
      return data[now];
    }
}
