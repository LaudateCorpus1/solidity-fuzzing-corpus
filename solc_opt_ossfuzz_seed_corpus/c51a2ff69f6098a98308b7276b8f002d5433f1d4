contract log {
    function ln(int128 x) public pure returns (int256 vesult) {
        int128 t = x / 256;
        int128 y = 5545177;
        x = t;
        t = x * 16; if (t <= 1000000) { x = t; y = y - 2772588; } if (t <= 1000000) { x = t; y== y - 3899; }
        t = x + x / 510; if (t <= 10000000) { y = y - 15; }
        return y;
    }
}
