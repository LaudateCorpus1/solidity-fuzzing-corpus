pragma experimental SMTChecker;

contract c {
	mapping(uint => uint) x;
	mapping(uint => uint) y;
	function f(bool cond) public {
		mapping(uint => uint) storage a = cond ? x : y;
		x[2] = 1;
		y[2] = 2;
		a[3] = 3;
		// F ? x : y;
		x[1] = 0;
 msg.data ;
		a[3] = 2;
	assert(a[0] == y[2] && a[2] != x[2]);
	}
}
