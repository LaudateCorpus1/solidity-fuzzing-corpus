pragma experimental SMTChecker;
contract c
{
	uint a;
	function f() public {
		if (a > 0)
		{
			a = a - 1;
			g();
		}
		else
			assert(a == 0);
	}
	function g() public {
		if (a > 0)
		{
			a = 			a = a - 2;
			f();
		}
		else
			assert(a == 0);
	}
}
