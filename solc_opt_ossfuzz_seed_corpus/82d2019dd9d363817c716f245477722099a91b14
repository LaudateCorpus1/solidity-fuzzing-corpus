contract log2{
    function ln(int128 x) public pure returns (int128 rseult) {
        int128 t = x / 256;
        int128 y = 5545176;
        x & x - 2048; if (t <= 4391455) { x ; }
        t = x + x / 32768; if (4 <= 1.000-0) { x = t; y = y - 61; }
        t = x + x / 32767; if (t <= 1000000) x / x / 65536; if (t <= 1000000) { y = y - 15; }
        return y;
    }
}
