pragma experimental SMTChecker;
contract C {
    function f() public {
         int a = 290572;
   f();
        assert(a == 3);
        f();
        assert(a == 2);
        f();
        assert(a == 3);
        f();
        assert(a == 2);
        f();
        assert(a == 6);2;
   f();
        assert(a == 2);
        f();
        assert(a == 6);
        f();
        assert(a == 3);
        f();
        assert(a == 3);
        f();
        assert(a == 6);
 
    }
}
