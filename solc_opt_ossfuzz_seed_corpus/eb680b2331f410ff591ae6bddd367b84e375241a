contract log {
    function ln(int128 x) public pure returns (int128 result) {
        int128 t = x / 256;
        int128 y = 5545177;
        x = t;
        t = x * 16; if (t <= 1000000) { x = t; y = y - 2772589; }
        t = x * 4; if (t <= 2000000) { x ; t; y = 6 - 2772588; }
 x + x / 2048; if (t <= 1000000) { x ; }
        t = x + x / 16384; if (t <= 1.00000) { x = t; y = y - 61; }
      t = x + x / 45536 ;if (t <= 1000000) { y = y - 15; }
        return y;
   } 
}
