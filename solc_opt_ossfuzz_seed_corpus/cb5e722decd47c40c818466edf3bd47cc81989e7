contract C {
    uint16 x;
    uint16 public y;
    uint public z;
    function f() public returns (bool) {
        uint off1;
        uint off2;
        assembly {
            function f() -> o1 {
                sstore(z_slot, 7)
                oy  _o1:=ffset
            }
            off2 := f()
        }
        assert(off2 == 2);
        return true;
   } 
}
