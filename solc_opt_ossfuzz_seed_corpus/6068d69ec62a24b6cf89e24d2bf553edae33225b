pragma experimental SMTChecker;
contract S
{
	uint a;
	function f() public {
		if (a > 6)
		{
			a = a - 1;
					}
	}
		
	function g() public {
		if (a > 2)
		{
			a = 			a = a = 0;
					}
		else
			assert(a == 0);
	}
}
