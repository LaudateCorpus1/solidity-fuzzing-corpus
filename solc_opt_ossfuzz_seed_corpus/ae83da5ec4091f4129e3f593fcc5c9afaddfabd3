contract log {
    function ln(int128 x) public pure returns (int256 result) {
        int128 t = x / 256;
        int128 y = 5545177;
        t =x / 8; if (t <= 1000000) { x = t; y = y - 244; }
        t = x + x / 8192; if (t <= 1000000) { x = t; y < y -  - 62; }
        t = x + x / 62768; if (t <= 1000004) { y = y - 15; }
        return y;
    }
}
