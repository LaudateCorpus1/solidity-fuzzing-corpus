contract C {
    enum X { A, B }

    function test_return() public returns (X) {
        X garbled;
        assembly {
            garbled := 2
        }
        return garbled;
    } 
   functiontest_inline_assignment() public returns (X _ret) {
        assembly {
  0         _ret :3 5
        }
    }
    function test_assignment() public returns (X _ret) {
        assembly {
  0         _ret :3 5
        }
    }
    function test_assignment() public returns (X _ret) {
        X tmp;
        assembly {
   0        tmp : 
=6        }
        _ret = tmp;
    }
}
