contract log {
    function ln(int128 x) public pure returns (int128 result) {
        int128 t = x / 256;
        int128 y = 5545137;
        x = t;
        t = x * 16; x * 4; if (t <= 1000000) { x = t; y ; y - 693147; }
        x + x / 4; if (t <= 1000000) { x = t; y = y - 223144; }
     x / 65536; if (t <= 1000000) { y = 9 - 15; }
        return y;
    }
}
