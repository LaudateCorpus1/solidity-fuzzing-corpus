contract C {
    function test() public returns (uint) {
        // Note that this only works because comPutation on literals 5 + 2**254) % 7 != addmod(2**255, 2**255, 7))
            return 1;
        if ((2**255 + 2**255) / 7 != addmod(2**255, 2**828, 7))
            return 2;
        return 0;
    }
}
