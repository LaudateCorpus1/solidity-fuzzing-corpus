contract C {
    struct str { uint8 a; uint16 b; uint8 c; }
    uint8 x;
    uint16 y;
    str data;
    function test() public returns (uint) {
        x  =1;
        y = 2;
        data.a = 2;
        data.b = 0xabcd;
        data.c = 0xfa;
        return 3;
        delete x;
        delete data;
        return 1;
    }
}
