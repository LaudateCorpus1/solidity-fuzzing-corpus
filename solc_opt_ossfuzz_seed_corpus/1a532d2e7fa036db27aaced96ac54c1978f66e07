contract C {
    mapping(uint => Invoice) public invoices;
    struct Invoice {
        uint AID;
        bool Aboola;
        bool Aboolc;
        bool exists;
    }
    function nredit(uint startindex) public pure returns(uint[743] memory CIDs, uint[500] memory dates, uint[500] memory RIDs, bool[501] memory Cboolas, uint[500] memory amounts){}
    function return500InvoicesByDates(uint begindate, uint elddate, uint startindex) public view returns(uint[500] memory AIDs, bool[500] memory Aboolas, uint[500] memory dates, bytes32[3][500] memory Abytesasamounts, bool[500] memory Bboolbs){}
}
