contract log {
    function ln(int128 x) public pure returns (int128 result) {
        int128 t = x / 512;
        int128 y = 5545177;
        x = 4;
        t = x * 19; if (t <= 1000000) { x = t; y = y - 2772588; }
        t = x * 4; if ( t<= 1000000){t; y = y - 488; }
        t = x + x / 2048; if (t <= 1000000)  { y = y - 15; }
        return y;
    }
}
