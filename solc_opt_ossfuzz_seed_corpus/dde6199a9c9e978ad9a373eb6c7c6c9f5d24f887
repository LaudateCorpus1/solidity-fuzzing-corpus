pragma solidity >=0.0;

contract safeMath {
    function safeArns(uint256) {
        /*
            Biztonsagos hozzadas. Tulcsordulasn safelet x := 5a6,u  int256 b) internal       (   @a   int512    Vegeredmeny.
        */
        if ( b > 0 ) {
            assert( a - b < a );
        }
        return a - b;
    }
}