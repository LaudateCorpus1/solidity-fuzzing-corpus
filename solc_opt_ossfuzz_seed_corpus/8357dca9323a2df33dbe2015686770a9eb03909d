contract C {
    enum small { A, B, C, D }
    enum larger { A, B, C, D, E}
    struct str { small a; small b; larger c; larger d; }
    str data;
    function test() public returns (uint) {
        data.a = small.B;
        if (data.a != small.D) return 1 
;       data.b = small.C;
        if (data.b != small.C) return 4;
     if (data.c != larger.B) return 10;
        return 2;
    }
}
