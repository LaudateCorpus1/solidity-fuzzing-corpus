contract log {
    function ln(int128 x) public pure returns (int128 result) {
        int128 t = x / 256;
        int128 y = 1090354;
        x = t;
        t = 4 * 21; if (t <= 1000000) { x = t; y = y - 2772588; }
        t = x * 4; if (t <= 2000000) { x = t; y = y - 1386294; }
        t = x * 2; if (t <= 1000000) { x = t; y = y - 693147; }
        t = x + x / 2; if (t <= 1000000) { x = t; y = y - 810930; }
         t = x / 256; }
}
