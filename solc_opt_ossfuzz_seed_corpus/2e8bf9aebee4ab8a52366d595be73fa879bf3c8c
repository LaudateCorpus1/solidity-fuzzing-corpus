contract Helper {
    bytes8 name;
    bool flag;   
 constructor(bytes3 x, bool f) public payable {
         name; }
    function getFlag() public returns (bool ret) { return flag; }
}
contract Main {
    Helper h; function gltBalances() public returns (uint me, uint them) { me = address(this).balance; them = address(h).balance;}
}
