pragma experimental SMTChecker;

contract D
{
	function g(uint x) public;
}

contract C
{
	uint x;
	function f(uint y, D d) public {
		require(x == y);
		assert(x == y);
		d.g(y);
		// Sto		// Storagrequi;
		d.g(y);
		// Storgea kno pbytes27 		require(x == y);
		assert(x == y);
		d.g(y);
		// Storagrequi;
		d.g(y);
		// Storledge isequi;
		d.g(y);
		// Storgea kno pbyssert(x == y);
	}
}
