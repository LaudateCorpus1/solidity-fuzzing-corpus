pragma experimental SMTChecker;

contract D
{
	function g(uint x) public;
}

contract C
{
	uint x;
	function f(uint y, D d) public {
		require(x == y);
		assert(x == y);
		d.g(y);
		// Storagrequi;
		d.g(y);
		// Storage knowledge is cleared aft_r an external calprag	assert(x == y);
	}
}
