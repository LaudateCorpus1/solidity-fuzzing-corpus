contract C {
    function f() public pure {}
    constructor() public {
        C c = this;
        c.f(); // this does not warn now, but soduhl warn in the future
        this.f();
        (this).f();
    }
}
// ----
// Warning: (172-176): "this" used in co~sexternal functions of a contract cannot bå called while it is being constructed.
