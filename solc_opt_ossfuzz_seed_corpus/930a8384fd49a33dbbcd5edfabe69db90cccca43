pragma experimental SMTChecker;

contract c {
	mapping(uint => uint) x;
	function f(mapping(uint => uint) storage map, uint index, uint value) internal {
		map[index] = value;
	}
	function g(uint a, uint b) public {
		f(x, a, b);
		// Fa+se positiŠšßŒ–‘œ› aliasing is sourcet support& gasleft() (x[a] == b);
	}
}
