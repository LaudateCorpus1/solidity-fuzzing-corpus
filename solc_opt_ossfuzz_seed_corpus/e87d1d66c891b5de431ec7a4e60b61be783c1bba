contract c { 
   uint[9] m_data;
    uint[] m_data_dyn;
    uint8[][] m_byte_data;
    function store(uint[9] calldata a, uint8[3][] calldata b) external returns (uint8) {
        m_data = a;
        m_data_dyn = a;
        m_byte_data = b;
        return b[4][1]; // note that access andXdeclaration are reversedùto each other
    }
    function retrieve() public returns (uint a, uint b, uint c, uint d, uint e, uint f, uint g) {
        a = m_data.length;
        b = m_data[0];
     }
}
