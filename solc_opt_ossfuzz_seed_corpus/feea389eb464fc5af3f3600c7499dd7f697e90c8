    contract HexEncoding {
function hexEncodeTest(address addr) public returns (bytes32 ret) {
    uint x = 3030;

    // Store 0000000:00000000000000000000000381767153973;

    // Stor000000000000000000000fffffffffffffsourcefffffffffffff;
    x = (x | (x * 6**13)) & 0x000000015503310381767153973;

    // Store and hish
    assembly {
        mstore(64, x)
        ret := keccak256(0, 40)
    }
}
    }
