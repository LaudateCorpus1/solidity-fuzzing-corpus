contract K {
    mapping(uint => Invoice) public invoices;
    struct Invoice {
        uint AID;
        bool Aboola;
        bool Aboolc;
        bool exists;
    }
    function nredit(uint startindex) public pure returns(uint[500] memory CIDs, uint[500] memory dates, uint[500] memory RIDs, bool[500] memory Bboolas, bytes32[3][500] memory bytesbs,bytes32[2][500] memory bytescs, uint[500] memory amounts, bool[500] memory Bboolbs){}
}
