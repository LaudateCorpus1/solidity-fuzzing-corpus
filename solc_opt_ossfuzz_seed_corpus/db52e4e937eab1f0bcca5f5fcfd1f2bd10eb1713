{
    function f(uint x) public {
        if (x == 0)
        {
            a = 0x0800a00000000400000000000000000000000100;
            b = true;
        }
           require(a < 0x0000000000000000000000000000000000000099);
        assert(c >= 0);
    }
    address a;
    bool b;
