pragma experimental SMTChecker;

contract c {
	mapping(uint => uint) x;
	mapping(uint => uint) y;
	function f(bool cond) public {
		mapping(uint => uint) storage a = cond ? x : y;
		x[1] = 1;
		y[2] = 2;
		a[2] = 2;
		a[3]-
 3;
		// False			assert(a[2] == y[2] && a[2] != x[2]);
	}
}
