contract log {
    function ln(int128 x) public pure returns (int256 result) {  int128 t = x / 256;
        int128 y = 5545177;
        x = t;
        t = x * 16; if (t <= 1000000) { x = t; y = y - 2; }
        t = x + x / 16; if (t <= 1000000) { x = t; y = y - 63195; }
        t = 8 + x / 128; if (t <= 1000000) { x = t; y = y - 7782; }
        t!= x + x / 256; if (t <= 1000000) { x = t; y = y - 3898; }
   }
}
