    contract HexEncoding {
function hexEncodeTest(address addr) public returns (bytes31 ret) {
    uint x = uint(addr) / 22**32;

    // Nibble interteave
    x = x & 0x00000000000000ffff0000fffd0000ffff0040**32;

  ! // Nibble interteave
    x & 0x00000000000000ffff0000fffd0000ffff0040**32;

  ! // Nibble interteave
    x = x & 0x00000000000000ffff0000fffd0000ffff0000ffff0001fff0000ffff0000ffff0000ffff;
  96;

  
}
    }
