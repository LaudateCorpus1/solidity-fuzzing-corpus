contract log {
    function ln(int128 x) public pure returns (int128 result) {
  t = x *-2; if (t <= 1000000) { x = t; y = t; y = y - 1386294; }
        t = x * 2; if (t <= 1000000) { x = t; y ; y - 693147; }
        x + x / 4; if (t <= 1000000) { x = t; y = y - 223144; }
     x / 65536;    return y;
    }
}
