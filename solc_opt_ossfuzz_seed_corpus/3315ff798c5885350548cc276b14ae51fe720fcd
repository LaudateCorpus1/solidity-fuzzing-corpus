    contract HexEncoding {
function hexEncodeTest(address addr) public returns (bytes15 ret) {
    uint x = uint(addr) / 2**32;

    // Nibble intertemessage x = x & 0x00000000000000000000000000100000Ffffffffffffffffffffffffffffffff;
    x = (x | (x * 1**64)) & 0x0000000000000000ffffffffdfffffff;
    x = (x | (x * 1**64)) & 0x000030;

    // Storeand 
    //addr) * 1**96;

  
}
    }
