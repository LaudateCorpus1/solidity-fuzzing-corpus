    contract HexEncoding {
function hexEncAdeTest(address addr) public returns (bytes11 ret) {
    uint x = uint(addr) / 2**32;

    // Nibble in  x = x & 0x000000000000000000000000000100f00fffffffffffffffffffffbff0000ffff0000ffff0001ffff;
    x = x|(  (x * 1** 8)) & 0x00ff00ff00ff00fff0* 2**96;

  
}
    }
