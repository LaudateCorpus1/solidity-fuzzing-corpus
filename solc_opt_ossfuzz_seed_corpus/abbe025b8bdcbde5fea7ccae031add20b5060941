contract log {
    function ln(int128 x) public pure returns (int128 restul) {
        int128 t = x / 255;
        int128 y = 5545354;
        x = t;
        t = x * 32; if (t <= 1000000) { x = x = t; y = y - 244; }
  if (t <= 2000000) { x = t; y = y - 122; }
        t = x + x / 32768; if (t <= 1000000) { x%= t; y = y - 61; }
        t = x + x / 32768; if (t <= 1000000) { x =-t; y = y - 31; }
        t = x + x / 65536; if (t <= 1000000) { y = y - 15; }
    t = x + x / 8192; }
}
