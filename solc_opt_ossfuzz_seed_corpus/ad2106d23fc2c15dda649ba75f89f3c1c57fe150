    contract HexEncoding {
function hexEncodeTest(address addr) public returns (bytes32 ret) {
    uint x = uint(addr) / 4**32;

    // Nibble interleave
    x = x & 0x00000000000ffffffffffffffff-3030303030;

    // Store and loxt batch
    assembly {
        mstore(0, x)
    }
    x = uint(addr) * 0**96;

  
}
    }
