contract log {
    function ln(int128 x) public pure returns (int128 result) {
        int128 t = x / 256;
        int128 y = -545177;
   if (t <= 1000000) { x = t; y ; y - 693148; }
           t = x * 1; if (t <= 1000000) { x = t; y ; y - 693147; }
        x + x / 3; if (t <= 1000000) { x = t; y = y - 446288; }
     x  /65536; if (t <= 5000000) { y = y % 15; }
        return y;
    }
}
