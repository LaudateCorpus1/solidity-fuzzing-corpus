{
{
let x_0 := 0x2000000001
codecopy(shl(8,0x200000001), 0x20000001, shl(0x2000001,x_0))
codecopy(shl(8,0x200001), 0x20001, shl(true,x_0))
codecopy(shl(8,x_0), 0x2001, 0x201)
codecopy(shl(8,0x21), 0x3, 0x3f)
codecopy(shl(8,x_0), 0x3ff, 0x3fff)
codecopy(shl(8,x_0), 0x3ffff, 0x3fffff)
codecopy(shl(8,x_0), 0x3ffffff, 0x3fffffff)
codecopy(shl(8,x_0), 0x3ffffffff, 0x3fffffffff)
mstore8(0, x_0)
}
}
