{
{
let x_0 := not(0x7fff)
codecopy(shl(8,x_0), 0x7ffff, 0x7fffff)
codecopy(signextend(8,x_0), 0x7ffffff, 0x7fffffff)
codecopy(shl(8,x_0), 0x7ffffffff, 0x7fffffffff)
codecopy(shl(8,0x7ffffffffff), 0x7fffffffffff, 0x7ffffffffffff)
codecopy(shl(8,x_0), 0x7fffffffffffff, 0x7ffffffffffffff)
codecopy(shl(8,x_0), 0x7fffffffffffffff, 0x7ffffffffffffffff)
codecopy(shl(8,x_0), 0x7fffffffffffffffff, 0x7ffffffffffffffffff)
codecopy(shl(8,x_0), 0x7fffffffffffffffffff, 0x7ffffffffffffffffffff)
}
}
